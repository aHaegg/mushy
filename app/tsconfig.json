    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "lib": ["dom", "es2015"],
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
